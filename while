#include <stdio.h>
#include <stdlib.h>
int main()
{
    int a,n,b;
    scanf("%d(1<=n<=30)",&n);
    b=1;
    while(b<=n)
    {
        a=1;
        while(a<=b)
        {
            printf("%d",b);
            printf(" ");
            a=a+1;
        }
        printf("\n");
        b=b+1;
    }
    return 0;
}
